{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "DenyAccessFromOutsideVPC",
            "Effect": "Deny",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::demo-secure-bucket-ef6a88a5",
                "arn:aws:s3:::demo-secure-bucket-ef6a88a5/*"
            ],
            "Condition": {
                "StringNotEquals": {
                    "aws:SourceVpc": "vpc-0783cb8485ffcf51d"
                },
                "ArnNotEquals": {
                    "aws:PrincipalArn": "arn:aws:iam::076675439156:user/admin"
                }
            }
        },
        {
            "Sid": "AllowAccessFromVPC",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": [
                "arn:aws:s3:::demo-secure-bucket-ef6a88a5",
                "arn:aws:s3:::demo-secure-bucket-ef6a88a5/*"
            ],
            "Condition": {
                "StringEquals": {
                    "aws:SourceVpc": "vpc-0783cb8485ffcf51d"
                }
            }
        }
    ]
}